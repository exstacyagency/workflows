# === KIE (server-side only) ===
KIE_API_BASE_URL=https://api.kie.ai
KIE_API_KEY=
KIE_CREATE_PATH=/api/v1/jobs/createTask
KIE_STATUS_PATH=/api/v1/jobs/recordInfo?taskId=

# Default provider selection
VIDEO_IMAGE_PROVIDER_ID=kie:nano-banana-pro

# Spend safety (set to 1 only when you explicitly want to spend credits)
KIE_LIVE_MODE=0

# Extra dev safety: require an explicit confirmation header on spend requests
# 1 = require header (recommended for dev), 0 = disabled
KIE_REQUIRE_SPEND_CONFIRMATION=1

# The header/value required when KIE_REQUIRE_SPEND_CONFIRMATION=1
KIE_SPEND_CONFIRM_HEADER=x-kie-spend-confirm
KIE_SPEND_CONFIRM_VALUE=1

# Status polling concurrency for multi-task (first/last) orchestration
VIDEO_IMAGES_STATUS_CONCURRENCY=4

# KIE / worker reliability
KIE_HTTP_TIMEOUT_MS=20000
KIE_POLL_INTERVAL_MS=2000
WORKER_JOB_MAX_RUNTIME_MS=1200000

# Dev: force rate limiter to enforce even outside production
FORCE_RATE_LIMIT=0
SECURITY_SWEEP=0
