{
  "name": "videgen",
  "version": "1.0.0",
  "description": "AI Ad Lab starter for customer research and ad ideation",
  "private": true,
  "type": "module",
  "scripts": {
    "env:db": "tsx scripts/print_env_db.ts",
    "db:studio": "bash scripts/run_prisma_studio.sh",
    "dev:cleanup-rate-limits": "tsx scripts/cleanup_rate_limits.ts",
    "dev:set-password": "tsx scripts/set_password.ts",
    "dev:create-script": "tsx scripts/create_script.ts",
    "billing:matrix": "node scripts/generate_billing_matrix.mjs",
    "dev": "next dev",
    "dev:with-env": "bash scripts/run_dev.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "worker:once": "RUN_ONCE=1 DOTENV_CONFIG_PATH=.env.local node -r dotenv/config ./node_modules/.bin/tsx ./workers/jobRunner.ts",
    "worker": "DOTENV_CONFIG_PATH=.env.local node -r dotenv/config ./node_modules/.bin/tsx ./workers/jobRunner.ts",
    "workers": "tsx workers.ts",
    "retry-worker": "ts-node -r tsconfig-paths/register workers/retryRunner.ts",
    "security:sweep": "node scripts/attacker_sweep.mjs",
    "security:login-lockout": "node scripts/login_lockout_test.mjs",
    "seed:ci": "node scripts/seed_ci.mjs",
    "auth:cleanup": "node scripts/cleanup_auth_throttle.mjs",
    "smoke:prod": "NODE_ENV=production npm run build && node scripts/prod_smoke.mjs"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@prisma/client": "5.22.0",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.6.1",
    "ioredis": "^5.8.2",
    "next": "14.2.5",
    "next-auth": "^4.24.13",
    "p-limit": "^7.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "stripe": "^20.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "20.14.10",
    "@types/react": "18.3.11",
    "autoprefixer": "^10.4.22",
    "eslint": "8.57.1",
    "eslint-config-next": "^14.2.5",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "5.6.3"
  }
}
