{
  "name": "videgen",
  "version": "1.0.0",
  "description": "AI Ad Lab starter for customer research and ad ideation",
  "private": true,
  "type": "module",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "test:prod-boot": "bash scripts/test-prod-boot.sh",
    "test:auth-isolation": "bash scripts/test-auth-isolation.sh",
    "e2e:deterministic": "bash scripts/e2e_deterministic.sh",
    "routes:manifest": "tsx scripts/route_inventory.ts",
    "baseline:check": "bash scripts/baseline_check.sh",
    "routes:check": "tsx scripts/routes_check.ts",
    "e2e": "bash scripts/smoke_membership.sh",
    "freeze:docs": "tsx scripts/freeze_require_docs.ts",
    "freeze:check": "tsx scripts/freeze_check.ts",
    "contract:bump-check": "tsx scripts/contract_bump_check.ts",
    "freeze:release": "bash scripts/freeze_release.sh",
    "buyout:docs": "node -e \"console.log('Docs live in docs/README_BUYOUT.md, docs/ARCHITECTURE.md, docs/EVALUATION.md, docs/CONTRACT_BUMP.md')\"",
    "golden:run": "SECURITY_SWEEP=1 KEEP_E2E_SERVER=1 npm run e2e:deterministic && SECURITY_SWEEP=1 npm run golden:driver && SECURITY_SWEEP=1 npm run golden:snapshot && SECURITY_SWEEP=1 npm run golden:compare && bash scripts/stop_e2e.sh",
    "golden:run:ci": "SECURITY_SWEEP=1 KEEP_E2E_SERVER=1 GOLDEN_OUT_DIR=/tmp/golden-output npm run e2e:deterministic && SECURITY_SWEEP=1 GOLDEN_OUT_DIR=/tmp/golden-output npm run golden:driver && SECURITY_SWEEP=1 GOLDEN_OUT_DIR=/tmp/golden-output npm run golden:snapshot && SECURITY_SWEEP=1 GOLDEN_OUT_DIR=/tmp/golden-output npm run golden:compare && bash scripts/stop_e2e.sh",
    "golden:update-baseline": "GOLDEN_OUT_DIR=/tmp/golden-output npm run golden:snapshot && node scripts/golden_update_baseline.js",
    "golden:driver": "tsx scripts/golden_driver.ts --input e2e/golden/input.json",
    "golden:snapshot": "tsx scripts/golden_snapshot.ts --outDir ${GOLDEN_OUT_DIR:-/tmp/golden-output} --projectId proj_test",
    "golden:compare": "tsx scripts/golden_compare.ts --out ${GOLDEN_OUT_DIR:-/tmp/golden-output}/summary.json --baseline e2e/golden/baseline/summary.json",
    "env:db": "tsx scripts/print_env_db.ts",
    "db:studio": "bash scripts/run_prisma_studio.sh",
    "smoke:membership": "bash scripts/smoke_membership.sh --help",
    "dev:cleanup-rate-limits": "tsx scripts/cleanup_rate_limits.ts",
    "dev:set-password": "tsx scripts/set_password.ts",
    "dev:create-script": "tsx scripts/create_script.ts",
    "bootstrap": "npx tsx scripts/bootstrap.ts",
    "bootstrap:dev": "tsx scripts/bootstrap-dev.ts",
    "billing:matrix": "node scripts/generate_billing_matrix.mjs",
    "dev:auth:reset": "node scripts/dev_auth_reset.mjs attacker@local.dev Attacker123",
    "dev": "next dev",
    "worker": "tsx workers/jobRunner.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run worker\"",
    "dev:with-env": "bash scripts/run_dev.sh",
    "build": "prisma generate && node -e \"const fs=require('fs');fs.rmSync('tsconfig.tsbuildinfo',{force:true});\" && next build",
    "prod:guardrails": "tsx scripts/prod_guardrails.ts",
    "verify:prod-routes": "tsx scripts/verify-prod-routes.ts",
    "verify:prod-guardrails": "bash scripts/verify-prod-guardrails.sh",
    "verify:prod-env": "bash scripts/verify-no-security-sweep-prod.sh",
    "start": "next start",
    "start:all": "concurrently \"npm start\" \"npm run worker\"",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "rls:enable": "npx prisma db execute --file scripts/enable_rls.sql",
    "worker:once": "RUN_ONCE=1 DOTENV_CONFIG_PATH=.env.local node -r dotenv/config ./node_modules/.bin/tsx ./workers/jobRunner.ts",
    "security:sweep": "node -r dotenv/config scripts/attacker_sweep.mjs",
    "security:login-lockout": "node scripts/login_lockout_test.mjs",
    "seed:ci": "node scripts/seed_ci.mjs",
    "auth:cleanup": "node scripts/cleanup_auth_throttle.mjs",
    "alpha:reset": "tsx scripts/alpha_reset.ts",
    "alpha:smoke": "MODE=alpha NODE_ENV=production bash scripts/alpha_smoke.sh",
    "smoke:prod": "NODE_ENV=production npm run build && node scripts/prod_smoke.mjs",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --passWithNoTests",
    "test:isolation": "npm run build && npm test -- tests/isolation.self-operator.test.ts",
    "dev:test": "NODE_ENV=test next dev",
    "start:test": "NODE_ENV=test next start",
    "bootstrap:test": "NODE_ENV=test tsx scripts/bootstrap-dev.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-s3": "^3.962.0",
    "@aws-sdk/s3-request-presigner": "^3.962.0",
    "@prisma/client": "7.3.0",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.6.1",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "mammoth": "^1.10.0",
    "next": "14.1.4",
    "next-auth": "^4.24.13",
    "node-fetch": "^3.3.2",
    "p-limit": "^7.2.0",
    "papaparse": "^5.5.3",
    "pdf-parse": "^1.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.7.0",
    "stripe": "^20.0.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "25.0.3",
    "@types/papaparse": "^5.5.2",
    "@types/react": "18.3.11",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "eslint": "8.57.1",
    "eslint-config-next": "^14.2.35",
    "jest": "^30.2.0",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "5.9.3"
  }
}
